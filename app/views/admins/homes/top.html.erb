<h1>管理者画面TOP</h1>

<div class="row-12">
    <div class="calendar col-xs-12">
      <%= month_calendar events: @events do |date, events| %>
	      <%= date.day %>
	      <% events.each do |event| %>
		    <div>
		      <%= link_to event.festival, admins_event_path(event) %>
		    </div>
          <% end %>
      <% end %>
    </div>
</div>
